Steam="*res://addons/godotsteam/steam.gd"
AchievementManager="*res://game/AchievementManager.gd"

[debug]

gdscript/warnings/integer_division=false

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
mouse_cursor/custom_image="res://assets/images/ui/cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 15, 5 )
mouse_cursor/tooltip_position_offset=Vector2( 20, 20 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

main_run_args="--allow_debug --is_steam"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godotsteam/plugin.cfg" )

